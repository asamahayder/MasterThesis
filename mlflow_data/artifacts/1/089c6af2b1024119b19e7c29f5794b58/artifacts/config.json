{
    "data_load": {
        "params": {
            "path_to_data_folder": "../Data/Ex-Vivo/"
        }
    },
    "preprocessing": {
        "params": {
            "pulse_window_size": 1500,
            "tukey_window_size": 250,
            "tukey_alpha": 0.80
        }
    },
    "feature_engineering": {
        "params": {
            "degree_of_polynomial": 22
        }
    },
    "model_training_and_evaluation": {
        "params": {
            "k_outer": 4,
            "k_inner": 5,
            "feature_count_threshold_inner": 2,
            "feature_count_threshold_outer": 2,
            "n_estimators": {
                "min": 200,
                "max": 200
            },
            "max_depth": {
                "min": 16,
                "max": 16
            },
            "min_samples_split": {
                "min": 6,
                "max": 6
            },
            "min_samples_leaf": {
                "min": 6,
                "max": 6
            },
            "bootstrap": [true],
            "criterion": ["gini"],
            "optuna_n_trials": 20
        }
    }
}
